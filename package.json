{
  "name": "json-schema-to-typescript-transpiler",
  "version": "1.0.0",
  "description": "A tool that converts JSON Schemas to TypeScript type definitions",
  "type": "module",
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "test": "bun test",
    "test:watch": "bun test --watch",
